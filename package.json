{
  "name": "pipboylib",
  "version": "1.0.1",
  "description": "The companion pipboy library to Fallout 4",
  "main": "lib/index.js",
  "scripts": {
    "compile": "rm -rf lib && babel src -q --out-dir lib",
    "postinstall": "npm run compile",
    "pretest": "npm run compile",
    "test": "babel-tape-runner test/*.js | faucet",
    "example": "babel-node example.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/RobCoIndustries/pipboylib.git"
  },
  "dependencies": {
    "babel-cli": "^6.2.0",
    "babel-plugin-transform-export-extensions": "^6.1.18",
    "babel-preset-es2015": "^6.1.18",
    "dissolve": "0.3.3",
    "lodash": "^3.10.1",
    "rx": "^4.0.7",
    "rx-node": "^1.0.1"
  },
  "devDependencies": {
    "babel-tape-runner": "^1.3.0",
    "faucet": "0.0.1",
    "tape": "^4.2.2"
  },
  "author": "Kyle Kelley <rgbkrk@gmail.com>",
